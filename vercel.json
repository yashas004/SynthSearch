{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "OPENROUTER_API_KEY": "@openrouter-api-key"
  },
  "functions": {
    "index.js": {
      "maxDuration": 30
    }
  }
}
